{
  "name": "booking-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev next dev -p 8083",
    "start": "next start -p 8083",
    "build": "npx prisma generate && next build",
    "typecheck": "tsc",
    "script:lint": "bash -e ./src/scripts/lint.sh",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "yarn prettier --write",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "prepare": "husky",
    "prismadb": "prisma generate",
    "prismamigrate": "prisma migrate dev",
    "prismamigrate:local": "dotenv -e .env.development -- prisma migrate dev --name init"
  },
  "dependencies": {
    "@clerk/nextjs": "^5.2.8",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@stripe/react-stripe-js": "^2.7.3",
    "@stripe/stripe-js": "^4.1.0",
    "@supabase/supabase-js": "^2.44.4",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "leaflet": "^1.9.4",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
    "prisma": "^5.17.0",
    "react": "^18.2.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-leaflet": "^4.2.1",
    "react-share": "^5.1.0",
    "recharts": "^2.12.7",
    "stripe": "^16.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.1",
    "world-countries": "^5.0.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-perfectionist": "^2.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^9.0.11",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "5.3.2"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9",
    "yarn": ">=1.22"
  }
}
